{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "error",
    "no-use-before-define": "error"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
